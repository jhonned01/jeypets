g, error } = useCategoriesData();\n\n  useEffect(() => {\n    const onScroll = (e) => {\n      const newShowFixed = window.scrollY > 200;\n\n      showFixed !== newShowFixed && setShowFixed(newShowFixed);\n    };\n    document.addEventListener(\"scroll\", onScroll);\n\n    return () => document.removeEventListener(\"scroll\", onScroll);\n  }, [showFixed]);\n\n  const renderList = (fixed) => (\n    <List fixed={fixed}>\n      {categories.map((category) => (\n        <Item key={category.id}>\n          <Category {...category} path={`/pet/${category.id}`} />\n        </Item>\n      ))}\n    </List>\n  );\n  if (loading) {\n    return <Loading />;\n  }\n  return (\n    <>\n      {renderList()} {showFixed && renderList(true)}\n    </>\n  );\n}\n","/home/jhonned01/Documentos/desesperado/proyecto/src/components/listOfCategories/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/photoCard/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/logo/logo.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/logo/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/styles/animation.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/styles/globalStyles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/LoadingComponent/loading.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/LoadingComponent/styled.js",["125"],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/photoCard/PhotoCard.js",["126"],"import React, { useRef, useEffect, useState, Fragment } from \"react\";\nimport { ImgWrapper, Img, Article } from \"./styles\";\nimport { useLocalStorage } from \"../../hooks/useLocalStorage\";\nimport FavButton from \"../favButton/FavButton\";\nimport { useLikePhoto } from \"../../container/useLikePhoto\";\nimport { Link } from \"@reach/router\";\n\nexport default function PhotoCard({ id, likes = 0, src }) {\n  const defaultImage =\n    \"https:/