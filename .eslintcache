.removeEventListener(\"scroll\", onScroll);\n  }, [showFixed]);\n\n  const renderList = (fixed) => (\n    <List fixed={fixed}>\n      {categories.map((category) => (\n        <Item key={category.id}>\n          <Category {...category} path={`/pet/${category.id}`} />\n        </Item>\n      ))}\n    </List>\n  );\n  if (loading) {\n    return <Loading />;\n  }\n  return (\n    <>\n      {renderList()} {showFixed && renderList(true)}\n    </>\n  );\n}\n","/home/jhonned01/Documentos/desesperado/proyecto/src/components/listOfCategories/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/photoCard/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/logo/logo.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/logo/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/styles/animation.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/styles/globalStyles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/LoadingComponent/loading.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/LoadingComponent/styled.js",["120"],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/photoCard/PhotoCard.js",["121"],"import React, { useRef, useEffect, useState, Fragment } from \"react\";\nimport { ImgWrapper, Img, Article } from \"./styles\";\nimport { useLocalStorage } from \"../../hooks/useLocalStorage\";\nimport FavButton from \"../favButton/FavButton\";\nimport { useLikePhoto } from \"../../container/useLikePhoto\";\nimport { Link } from \"@reach/router\";\n\nexport default function PhotoCard({ id, likes = 0, src }) {\n  const defaultImage =\n    \"https://images.unsplash.com/photo-1518791841217-8f162f1e1131?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=800&q=60\";\n\n  const element = useRef(null);\n  const [show, setShow] = useState(false);\n  const key = `like-${id}`;\n\n  const [toggleLike] = useLikePhoto();\n  const [liked, setLiked] = useLocalStorage(key, false);\n\n  useEffect(() => {\n    Promise.resolve(\n      typeof window.IntersectionObserver !== \"undefined\"\n        ? window.IntersectionObserver\n        : import(\"intersection-observer\")\n    ).then(() => {\n      const observer = new window.IntersectionObserver((entries) => {\n        const { isIntersecting } = entries[0];\n\n        if (isIntersecting) {\n          setShow(true);\n          observer.disconnect();\n        }\n      });\n\n      observer.observe(element.current);\n    });\n  }, [element]);\n\n  const Image = src ? src : defaultImage;\n\n  const handleFavButtonClick = () => {\n    console.log(`id de la photo ${id}`);\n    setLiked(!liked);\n    toggleLike({ variables: { input: { id: id } } });\n  };\n\n  return (\n    <Article ref={element}>\n      {show && (\n        <>\n          <Link to={`/detail/${id}`}>\n            <ImgWrapper>\n              <Img src={src} alt=\"photoCard\"></Img>\n            </ImgWrapper>\n          </Link>\n          <FavButton\n            liked={liked}\n            likes={likes}\n            onClick={handleFavButtonClick}\n          />\n        </>\n      )}\n    </Article>\n  );\n}\n","/home/jhonned01/Documentos/desesperado/proyecto/src/components/listOfPhotoCards/ListOfPhotoCards.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/hooks/useLocalStorage.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/hoc/useGetPhoto.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/container/PhotoCardWithQuery.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/favButton/FavButton.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/components/favButton/styles.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/container/useLikePhoto.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/pages/home.js",[],"/home/jhonned01/Documentos/desesperado/proyecto/src/pages/Detail.js",[],"/home/jhonned01/Documentos/desesperado/proyecto